7-6
#include<bits/stdc++.h>
using namespace std;
int f(int a, int b) {
    if(a%b==0) return b;
    return f(b, a%b);
}
int main()
{
    int a, b;
    cin>>a>>b;
    cout<<f(a,b);
}


7-7
#include<bits/stdc++.h> 
using namespace std; 
int dp[9999999];
unsigned long long f(int n){
    if(n==1 || n==2) return 1;
    else if(dp[n]!=-1){
        return dp[n];
    }
    dp[n]=f(n-1)+f(n-2);
    return dp[n];
}
int main() {
    dp[1]=dp[2]=1;
    for(int i=3; i<9999999; ++i) dp[i]=-1;
    int i, n;
    cin>>n;
    while(n--){
        cin>>i;
        cout<<f(i)<<'\n';
    }
}


