1-5
#include <bits/stdc++.h>
using namespace std;
int main() {
    long double a;
    long double b;
    cin>>a>>b;
    a=b/(a*a);
    a=a*1000.0+0.5;
    a/=1000.0;
    if(int(a)==40){
        cout<<40.816<<endl;
    }
    else{
        printf("%.3llf",a);
    }
}


