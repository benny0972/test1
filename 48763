7-4
#include<bits/stdc++.h> 
using namespace std; 
long long int sum=1;
unsigned long long f(int n){
    if(n==1) return 1;
    int h=f(n-1)+n;
    sum+=h;
    return h;
}
int main() {
    int i;
    cin>>i;
    f(i);
    cout<<sum;
}


