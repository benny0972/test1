}


2-2
#include <bits/stdc++.h>
using namespace std;
int main() {
    int a;
    cin>>a;
    if(a<=10) cout<<a*8<<endl;
    else if(a>10 and a<=15) cout<<80+(a-10)*2<<endl;
    else if(a>15 and a<24) cout<<90+(a-15)<<endl;
    else cout<<99<<endl;
}


