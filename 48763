1-9
#include <bits/stdc++.h>
using namespace std;
int main() {
    string a;
    cin>>a;
    if(a.length()>=3){
        int b=a.length();
        printf("個位數: %c, 十位數: %c, 百位數: %c",a[b-1],a[b-2],a[b-3]);
    }
    else if(a.length()==2){
        printf("個位數: %c, 十位數: %c, 百位數: 0",a[1],a[0]);
    }
    else if(a.length()==1){
        printf("個位數: %c, 十位數: 0, 百位數: 0",a[0]);
    }
}


