}


3-9
#include<bits/stdc++.h> 
using namespace std; 
int main() 
{
    int n, o, sum=0;
    cin>>n;
    for(int i=0; i<n; ++i)
    {
        if(i<n-1) while(cin>>o) {sum+=abs(o-2); if(getchar()=='\n') break;}
        else while(cin>>o) {sum+=abs(o-2); cin.ignore();}
        cout<<sum<<'\n';
        sum=0;
    }
}


