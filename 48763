1-2
#include <iostream>

using namespace std;

int main()
{
    cout << "I am sorry." << endl;
    cout << "I'm sorry." << endl;
    cout << "I'm "<<'"'<<"Sorry"<<'"'<<"." << endl;
    cout << "I'm "<<'"'<<"Sorry"<<'"'<<". "<<"\\"<<"(O^w^O)/" << endl;
    return 0;
}


