#include<bits/stdc++.h>
using namespace std;
long long f(int n)
{
    if(n==0) return 1;
    else  return n+f(n-1);
}
int main()
{
   int n;
   while(cin>>n)
   {
      if(n<0) break;
      cout<<f(n)<<'\n';
   }
}


7-10
#include<bits/stdc++.h> 
using namespace std; 
int f91(int n){
    if(n<=100) return f91( f91( n+11) );
    return n-10;
}
int main() {
    int i;
    while(cin>>i){
        if(i==0) break;
        cout<<"f91("<<i<<") = "<<f91(i)<<'\n';
    }
}


7-11
#include<bits/stdc++.h> 
using namespace std; 
queue<int> q;
inline int r();
int main() {
    string o;
    while(cin>>o){
        if(o=="f") q.push(2000);
        else if(o=="g") q.push(3000);
        else q.push(stoi(o));
    }
    cout<<r();
}
int r(){
    int v= q.front(); q.pop();
    if(v<=1000) return v;
    else if(v==2000) return (r()<<1)-1;
    else{
        int x=r();
        int y=r();
        return x+(y<<1)-3;
    }
}


7-12(X


7-13
#include<bits/stdc++.h> 
using namespace std; 
int main() 
{
    cin.tie(0), cout.tie(0), ios::sync_with_stdio(0);
    int n, weight[10000], value[10000], dp[10001][101];   
    cin>>n;
    for(int i=0; i<n; ++i) cin>>weight[i]>>value[i];
    for(int i=0; i<10001; ++i) dp[i][0]=0;
    for(int i=0; i<101; ++i) dp[0][i]=0;
    for(int i=1; i<=n; ++i)
    {
        for(int j=1; j<=100; ++j)
        {
            if(weight[i-1]>j) dp[i][j]=dp[i-1][j];
            else dp[i][j]=max(dp[i-1][j], dp[i-1][j-weight[i-1]]+value[i-1]);
        }
    }
    cout<<dp[n][100];
}
