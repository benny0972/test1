#include <bits/stdc++.h>
using namespace std;
int main()
{
    string a;
    int b;
    getline(cin,a);
    b=a.length();
    for(unsigned int i=0;i<b;i++){
        if(i%2!=0){
            cout<<"*";
        }
        if(i%2==0){
            cout<<a[i];
        }
    }
    return 0;
}


