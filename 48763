3-6
#include <bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    int a;
    map<char,int> M=
    {
        {'A',10},
        {'B',11},
        {'C',12},
        {'D',13},
        {'E',14},
        {'F',15},
        {'G',16},
        {'H',17},
        {'I',34},
        {'J',18},
        {'K',19},
        {'L',20},
        {'M',21},
        {'N',22},
        {'O',35},
        {'P',23},
        {'Q',24},
        {'R',25},
        {'S',26},
        {'T',27},
        {'U',28},
        {'V',29},
        {'W',32},
        {'X',30},
        {'Y',31},
        {'Z',33}
    };
    cin>>s;
    a=M[s[0]]/10+(M[s[0]]-M[s[0]]/10*10)*9;
    for(short i=1,j=8;i<9;i++,j--)
    {
        a+=(s[i]-48)*j;
    }
    a+=(s[9]-48);
    if(a%10==0)
    {
        cout<<"real";
    }
    else
    {
        cout<<"fake";
    }
}


