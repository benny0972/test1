3-11
#include<bits/stdc++.h> 
using namespace std; 
int main() 
{
    string table={"GHIJABCOPQRSTUDEFVWXYZKLMNghijklabpqrstcdefmnxyzouvw ,(:)'\".-!?[]{}#%&*0123456789"};
    string s1, s2;
    getline(cin,s1);
    for(char c: s1) s2.push_back(table[(table.find(c)+7)%table.size()]);
    cout<<s2;
}


3-12
#include<bits/stdc++.h> 
using namespace std; 
int main()
{
    cin.tie(), cout.tie(0), ios::sync_with_stdio(0);
    string table={"GHIJABCOPQRSTUDEFVWXYZKLMNghijklabpqrstcdefmnxyzouvw ,(:)'\".-!?[]{}#%&*0123456789"};
    string s1, s2;
    getline(cin,s1);
    for(char c: s1) cout<<table[(table.find(c)+table.find(*s1.rbegin()))%table.size()];
}


3-13
#include<bits/stdc++.h> 
using namespace std; 
int main()
{
    bool b;
    string s1, s2;
    while(getline(cin,s1))
    {
        b=0;
        getline(cin,s2);
        if(s1.size()==s2.size())
        {
            sort(s1.begin(), s1.end());
            sort(s2.begin(), s2.end());
            for(int i=0; i<s1.size(); ++i) if(s1[i]!=s2[i]) {b=1; break;}
            if(b) cout<<"No\n";
            else cout<<"Yes\n";
        }
        else cout<<"No\n";
    }
}


