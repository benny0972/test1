3-3
#include <bits/stdc++.h>
using namespace std;
int main() {
    short int time[6];
    for(char i=0;i<4;i++)
    {
        cin>>time[i];
        cin.ignore();
    }
    
    if(time[0]*60+time[1]<time[2]*60+time[3])
    {
        printf("%d",time[2]*60+time[3]-time[0]*60-time[1]);
    }
    else
    {
       printf("%d",1440+time[2]*60+time[3]-time[0]*60-time[1]); 
    }
}


3-4
#include <bits/stdc++.h>
using namespace std;
int main() {
    string s;
    int score=0;
    cin>>s;
    transform(s.begin(),s.end(),s.begin(),::toupper);
    for(int c : s)
    {
        score+=(c-64);
    }
    printf("%d",score);
}


