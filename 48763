1-12
#include <bits/stdc++.h>
using namespace std;
int main() {
    int a[6],t,h,m,s;
    for(char i=0;i<6;i++){
        cin>>a[i];
        cin.ignore();
    }
    if(a[3]<a[0]){
        a[3]+=24;
    }
    t=(a[3]*3600+a[4]*60+a[5])-(a[0]*3600+a[1]*60+a[2]);
    h=t/3600;
    m=(t-h*3600)/60;
    s=t-(h*3600+m*60);
    if(h<10){
        cout<<0;
    }
    cout<<h<<':';
    if(m<10){
        cout<<0;
    }
    cout<<m<<':';
    if(s<10){
        cout<<0;
    }
    cout<<s<<'\n';
}


1-13
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int r=0;r<t;r++)
    {   
        double a,b,c;
        cin>>a>>b>>c;
        double d=abs(b*b-4*a*c);
        if(b*b-4*a*c>=0)
        {
            if(int(sqrt(d))==sqrt(d))
            {
            cout<<"Yes"<<endl;
            }
            else
            {
            cout<<"No"<<endl;
            }
        }
        else
        {
           cout<<"No"<<endl; 
        }
    }
    return 0;
}


