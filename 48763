2-10
#include <bits/stdc++.h>
using namespace std;
int main()
{
    unsigned long long n,a,b,d;
    cin>>n>>a>>b;
    if(a<n and a<=b)
    {
        cout<<-1<<endl;
    }
    else
    {
        if(n-a==0)
        {
            cout<<1<<endl;
        }
        else
        {
            if((n-a)%(-b+a)==0)
            {
                cout<<(n-a)/(-b+a)+1<<endl;
            }
            else
            {
                cout<<(n-a)/(-b+a)+2<<endl;
            }
        }
    }
    return 0;
}


2-11
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        long p;
        bool a=0;
        cin>>p;
        if(p<=1)
        {
            a=1;
        }
        else
        {
           for(int r=2;r<(int)sqrt(p);r++)
           {
                if(p%r==0)
                {
                    a=1;
                    break;
                }
            } 
        }
        if(a)
        {
            cout<<"非質數"<<endl;
        }
        else
        {
            cout<<"質數"<<endl;
        }
    }
    return 0;
}


2-12
#include <iostream>
using namespace std;
int main()
{
    int foot;
    while(cin>>foot)
    {
        if(foot==0)
        {
            break;
        }
        if(1)
        {
            int MAX=foot/4+(foot-foot/4*4)/2;
            int MIN=foot/2;
            cout<<MAX<<' '<<MIN<<endl;
        }
        else
        {
            cout<<"0 0"<<endl;
        }
    }
}


