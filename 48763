1-5
#include <bits/stdc++.h>
using namespace std;
int main() {
    long double a;
    long double b;
    cin>>a>>b;
    a=b/(a*a);
    a=a*1000.0+0.5;
    a/=1000.0;
    if(int(a)==40){
        cout<<40.816<<endl;
    }
    else{
        printf("%.3llf",a);
    }
}


1-6
#include <iostream>
#include <cstdio>
using namespace std;
int main()
{
    unsigned long a;
    int b;
    cin>>a;
    b=a/3;
    if(a-b*3)
    {
        printf("第 %d 組",b+1);
    }
    else
    {
        printf("第 %d 組",b);
    }
}


1-7
#include <iostream>
#include <cstdio>
using namespace std;
int main(){
    int a;
    cin>>a;
    if(a==0){
        cout<<1<<endl;
    }
    else if(a==1) {
        cout<<2<<endl;
    }
    else{
        cout<<0<<endl;
    }
}


1-8
#include <bits/stdc++.h>
using namespace std;
int main() {
    int a;
    cin>>a;
    a-=15;
    if(a<0){
        cout<<24+a<<endl;
    }
    else{
        cout<<a<<endl;
    }
}


1-9
#include <bits/stdc++.h>
using namespace std;
int main() {
    string a;
    cin>>a;
    if(a.length()>=3){
        int b=a.length();
        printf("個位數: %c, 十位數: %c, 百位數: %c",a[b-1],a[b-2],a[b-3]);
    }
    else if(a.length()==2){
        printf("個位數: %c, 十位數: %c, 百位數: 0",a[1],a[0]);
    }
    else if(a.length()==1){
        printf("個位數: %c, 十位數: 0, 百位數: 0",a[0]);
    }
}


