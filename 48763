#include <bits/stdc++.h>
using namespace std;
int main()
{
    int y;
    cin>>y;
    if(y%4==0)
    {
        if(y%400==0)
        {
            cout<<"a leap year";
        }
        else if(y%400!=0 and y%100==0)
        {
            cout<<"a normal year";
        }
        else
        {
            cout<<"a leap year";
        }
    }
    else
    {
        cout<<"a normal year";
    }
}


