using namespace std; 
long long int dp[51]={1,1};
int main() {
    for(int i=2; i<51; ++i) dp[i]=dp[i-1]+dp[i-2];
    int i;
    while(cin>>i){
        if(i==0) break;
        cout<<dp[i]<<endl;
    }
}


